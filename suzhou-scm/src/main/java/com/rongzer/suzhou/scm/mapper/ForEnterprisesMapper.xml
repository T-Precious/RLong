<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rongzer.suzhou.scm.dao.ForEnterprisesMapper">
    <select id="selectAllForEnterprises" resultType="com.alibaba.fastjson.JSONObject">
        select
        id,enterprise_name,social_credit_code,latitude,longitude
        FROM
        Four_Enterprises
    </select>
    <select id="selectForEnterprisesBySocialCreditCode" resultType="com.alibaba.fastjson.JSONObject">
        select
        *
        FROM
        Four_Enterprises
        where social_credit_code=#{socialCreditCode}
    </select>
    <update id="updateForEnterprises">
        update Four_Enterprises
        <set>
            longitude=#{longitude},
            latitude=#{latitude},
        </set>
        where social_credit_code=#{socialCreditCode}
    </update>
</mapper>
